{
  "version": 2,
  "rewrites": [
    {
      "source": "/qr-generator",
      "destination": "/qr-generator.html"
    },
    {
      "source": "/qr-generator.html",
      "destination": "/qr-generator.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/home/(.*)",
      "destination": "/",
      "permanent": true
    }
  ]
}
